{
  "name": "GoodTranslate",
  "description" : "A Chrome Extension To Translate",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "good.html",
    "default_icon": "good.png"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+1",
        "mac": "MacCtrl+1"
      },
      "description": "Opens good.html"
    },
    "translate": {
      "suggested_key": {
        "default": "Alt+3",
        "mac": "MacCtrl+3"
      },
      "description": "translate"
    }
  },
  "permissions": [   
    "tabs",
    "notifications",
    "http://*/",
    "https://*/", 
    "activeTab"
  ]
}
