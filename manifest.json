{
  "name": "GoodTranslate",
  "description" : "A Chrome Extension To Translate",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "good.html",
    "default_icon": "good.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Shift+T",
        "mac": "Shift+T"
      },
      "description": "Opens good.html"
    }
  }
}
